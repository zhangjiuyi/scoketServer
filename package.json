{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "win": "electron-builder --dir -w"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "build": {
    "productName": "server",
    "appId": "your.id",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "extraResources": {
        "from": "./app-update.yml",
        "to": "./b.txt"
      }
    }
  },
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "ws": "^7.1.2"
  }
}